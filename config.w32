ARG_ENABLE("printer", "Printer Support", "no");

if (PHP_PRINTER != "no") {
	CHECK_LIB('winspool.lib', 'printer');
	CHECK_LIB('gdi32.lib', 'printer');
	EXTENSION("printer", "printer.c", PHP_PRINTER_SHARED);
	AC_DEFINE('HAVE_PRINTER', 1, 'Have Printer support', false);
}
